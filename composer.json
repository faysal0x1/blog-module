{
    "name": "faysal0x1/blog-module",
    "description": "Blog management module for Laravel with categories, tags, comments, and likes.",
    "type": "laravel-module",
    "license": "MIT",
    "keywords": [
        "laravel",
        "module",
        "blog"
    ],
    "authors": [
        {
            "name": "Faysal",
            "homepage": "https://github.com/faysal0x1"
        }
    ],
    "require": {
        "php": "^8.2",
        "nwidart/laravel-modules": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Modules\\Blog\\": ""
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Modules\\Blog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "module-dir": "Modules"
    }
}
